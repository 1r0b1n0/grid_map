<?xml version="1.0"?>
<package>
  <name>grid_map_filters</name>
  <version>0.1.0</version>
  <description>library for filtering gri maps.</description>
  <maintainer email="pfankhauser@ethz.ch">Peter Fankhauser</maintainer>
  <license>BSD</license>
  <url type="website">http://github.com/ethz-asl/grid_map</url>
  <url type="bugtracker">http://github.com/ethz-asl/grid_map/issues</url>
  <author email="martiwer@student.ethz.ch">Martin Wermelinger</author>
  <author email="pfankhauser@ethz.ch">Peter Fankhauser</author>
  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>cmake_modules</build_depend>
  <build_depend>grid_map</build_depend>
  <build_depend>grid_map_core</build_depend>
  <build_depend>grid_map_msg</build_depend>
  <build_depend>filters</build_depend>
  <run_depend>grid_map</run_depend>
  <run_depend>grid_map_core</run_depend>
  <run_depend>grid_map_msg</run_depend>
  <run_depend>filters</run_depend>
  <export>
    <filters plugin="${prefix}/filter_plugins.xml" />
  </export>
</package>
